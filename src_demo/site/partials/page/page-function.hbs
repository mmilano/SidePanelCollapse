{{!--
**********
page element
page function
establish some of the global values for the page, and call page specific function(s)

@param {string} page: page id value

**********
--}}

<script>
{{#with (lookup . page)}}
    const pageID = "{{id}}";
    document.addEventListener("DOMContentLoaded", site.pageRouter(pageID), false);
{{/with}}
</script>
